 {:db/ident :scheduled-transaction-item/account
  :db/valueType :db.type/ref
  :db/cardinality :db.cardinality/one
  :db/doc "The account affected by this transaction item"}
 {:db/ident :scheduled-transaction-item/action
  :db/valueType :db.type/keyword
  :db/cardinality :db.cardinality/one
  :db/doc "The account action to be taken for this item"}
 {:db/ident :scheduled-transaction-item/quantity
