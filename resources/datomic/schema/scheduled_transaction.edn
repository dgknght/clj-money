[{:db/ident :scheduled-transaction/entity
  :db/valueType :db.type/ref
  :db/cardinality :db.cardinality/one
  :db/doc "The entity to which the scheduled transaction belongs"}
 {:db/ident :scheduled-transaction/start-date
  :db/valueType :db.type/instant
  :db/cardinality :db.cardinality/one
  :db/doc "The date on which the scheduled transaction starts to be evaluated"}
 {:db/ident :scheduled-transaction/last-occurrence
  :db/valueType :db.type/instant
  :db/cardinality :db.cardinality/one
  :db/doc "The date of the last transaction produced by this schedule"}
 {:db/ident :scheduled-transaction/end-date
  :db/valueType :db.type/instant
  :db/cardinality :db.cardinality/one
  :db/doc "The date after which the scheduled transaction is no longer evaluated"}
 {:db/ident :scheduled-transaction/enabled
  :db/valueType :db.type/boolean
  :db/cardinality :db.cardinality/one
  :db/doc "Indicates whether or not the scheduled transaction should be evaluated"}
 {:db/ident :scheduled-transaction/description
  :db/valueType :db.type/string
  :db/cardinality :db.cardinality/one
  :db/doc "The transaction description"}
 {:db/ident :scheduled-transaction/memo
  :db/valueType :db.type/string
  :db/cardinality :db.cardinality/one
  :db/doc "An extra note about the transaction"}
 {:db/ident :scheduled-transaction/period
  :db/valueType :db.type/tuple
  :db/tupleTypes [:db.type/long :db.type/keyword]
